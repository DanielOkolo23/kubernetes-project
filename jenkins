pipeline {
    agent any
    environment {
        AWS_ACCESS_KEY_ID = credentials('AWS_ACCESS_KEY_ID')
        AWS_SECRET_ACCESS_KEY = credentials('AWS_SECRET_ACCESS_KEY')
        AWS_DEFAULT_REGION = "eu-west-2"
    }
    
 
        stage("Deploy voting-app to EKS") {
            steps {
                script {
                    dir('votingapp') {
                        sh "kubectl apply -f votingapp.yaml"
                    }
                }
            }
        }

       
